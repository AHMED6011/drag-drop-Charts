{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "rtl.config.js"],
  "compilerOptions": {
    "lib": ["es2017"],
    "ignoreDeprecations": "5.0",
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": [
      "vue3-toastify/global"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
